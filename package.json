{
  "name": "github-watch-all-repos",
  "version": "1.0.0",
  "description": "Watch all repositories belonging to an individual user/organization.",
  "author": "PythonCoderAS",
  "bin": {
    "github-watch-all-repos": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PythonCoderAS/github-watch-all-repos.git"
  },
  "keywords": [
    "github",
    "watch",
    "repositories"
  ],
  "bugs": {
    "url": "https://github.com/PythonCoderAS/github-watch-all-repos/issues"
  },
  "homepage": "https://github.com/PythonCoderAS/github-watch-all-repos#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.9.0",
    "@oclif/plugin-help": "^5.1.12",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "chai": "^4.3.6",
    "eslint": "^8.16.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^13.1.1",
    "mocha": "^10.0.0",
    "oclif": "^3.0.1",
    "shx": "^0.3.3",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "github-watch-all-repos",
    "dirname": "github-watch-all-repos",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "types": "dist/index.d.ts"
}
